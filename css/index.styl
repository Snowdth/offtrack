@import 'nib'
@import url('https://fonts.googleapis.com/css?family=PT+Sans');


html
body
	margin 0
	height 100%
html
	height 100%
	width 100%
body
	background white
	font-family 'PT Sans'
	font-style normal
	-webkit-font-smoothing antialiased
	
li
	list-style none
ul
	padding 0
a
	cursor pointer
	text-decoration none
article
aside
details
figcaption
figure
footer
header
hgroup
menu
nav
section
	display: block;

h2
h1
p
h4
h5
	margin 0
h5
	font-size 13px
	line-height 22px
	color #DAD1D1
h2
	color #585858
	font-size 35px
h3
	color #585858
	font-size 30px
h4
	color #585858
	font-size 19px
input
	outline 0px

	
::-webkit-scrollbar
	background #f4f4f4
	width 8px
	height 8px
::-webkit-scrollbar-thumb
	background #222
	width 8px

	

.header
	padding 38px 15px 0px
.top-bar
	margin 0 auto
	max-width 1295px
	width 100%
	color white
	display flex
.logo-container
	margin-left 15px
	min-width 195px
.logo
	cursor pointer
	background-image url('../img/logo.png') 
	background-repeat no-repeat
	height 50px
	width auto
	display block
.appbar-nav
	position relative
	width 871px
.appbar-nav-menu
	font-size 16px
	display flex
	justify-content center
	li
		min-width 76px
		margin-left 30px
		a
			border-bottom 2px solid
			border-color transparent
			font-size 14px
			color #FAF3F3
			display inline-block
			height 23px
		img
			padding 0px 0 2px 8px
.appbar-nav-section:hover
	transition 0.15s
	border-color #959292
	box-sizing border-box
.section-option:hover
	transition 0.1s
	font-weight 600
.appbar-services
	display inherit
	border-color transparent !important
	padding 0 0 15px 0
	position relative
.appbar-industries
	display inherit
	border-color transparent !important
	padding 0 0 15px 0
	position relative
.appbar-services:hover
	.dropdown-menu
		display block
	.dropdown-menu-industries
		display block
.appbar-services:hover
	.dropdown-menu
		display block
	.dropdown-menu-servicies
		display block
.dropdown-menu
	display none
	min-width 172px
	right -10%
	margin-top -4px
	z-index 101
	margin-bottom 20px
	background-color rgba(190, 190, 193,0.6)
	padding 10px 0
	border-radius 3px
	position absolute
	top 100%
	//rgba(190, 190, 193,0.6)   rgba(50,50,50,0.4)
	li
		position relative
	&::after
		content ""
		border 8px solid transparent
		border-bottom 8px solid rgba(190, 190, 193,0.6)
		position absolute
		top -16px
		right 21px
		left auto
.dropdown-menu-servicies
	min-width 172px
.dropdown-menu-industries
	min-width 222px
.subsection
	display none
	position absolute
	top 50%
	left -10%
	height 6px
	width 6px
	transform translateY(-88%)
	border-radius 3px
	background-color #3c75d8
.subsection-active
	font-weight 600
	.subsection
		display block !important
.section-active
	border-bottom 2px solid !important
	border-color #959292 !important
	box-sizing border-box
.section-inactive
	border-bottom 0 !important
	border-color #959292 !important


